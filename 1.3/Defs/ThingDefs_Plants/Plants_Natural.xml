<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="PlantBase">
		<defName>TalMozuuk</defName>
		<label>mozuuk</label>
		<description>A talyxian organism that is similar to edible seaweed, grass, and smokeleaf.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.175</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plants/Mozuuk</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shaderType>TransparentPlant</shaderType>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>14</pathCost>
		<ingestible />
		<plant>
			<dieIfLeafless>true</dieIfLeafless>
			<maxMeshCount>4</maxMeshCount>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawMozuuk</harvestedThingDef>
			<harvestYield>2</harvestYield>
			<sowTags>
				<li>Water</li>
				<!--
				<li>Ground</li>
				<li>Hydroponic</li>
				-->
			</sowTags>
			<growDays>3</growDays>
			<visualSizeRange>0.3~0.8</visualSizeRange>
			<purpose>Food</purpose>
			<humanFoodPlant>true</humanFoodPlant>
			<!-- Not meant to spawn naturall - yet
			<wildBiomes>
				<Desert>25</Desert>
				<TemperateForest>0.25</TemperateForest>
				<AridShrubland>2</AridShrubland>
			</wildBiomes>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>200</wildClusterWeight>
			<wildOrder>2</wildOrder>
			-->
			
			<!-- Mozuuk is very resilient-->
			<harvestWork>65</harvestWork>
			<dieFromToxicFallout>false</dieFromToxicFallout>
			<dieIfNoSunlight>false</dieIfNoSunlight>
			<fertilityMin>0.1</fertilityMin>
			<fertilitySensitivity>0.05</fertilitySensitivity>
			<topWindExposure>0.02</topWindExposure>
		</plant>
	</ThingDef>
	
